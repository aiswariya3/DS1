#include<stdio.h>
int main()
{
	int i,n,a[50],F,L,mid,key,flag;
	printf("enter n values");
	scanf("%d",&n);
	printf("enter the array elements:");
	scanf("%d",&a[i]);
	printf("enter the key value");
	scanf("%d",&key);
	flag=0;
	while( F<=L)
	{
		mid=((F+L)/2);
		if (a[mid]==key)
		{
			flag=1;
			break;
			{
				else if a[mid]<key);
				F=mid+1;
				else if(a[mid]>key);
				L=mid-1;
				if(flag==1)
				printf("found",key);
				else
				printf("not found",key);
				return 0;
			}
				
			}
		}
	}
}
